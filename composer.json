{
    "name": "php-etl/phpspec-extension",
    "description": "Extension for testing FastMap and ETL components with PHPSpec testing framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "phpspec/phpspec": "^6.0",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "doctrine/inflector": "^1.3",
        "nikic/php-parser": "^4.2",
        "symfony/expression-language": "^3.4 || ^4.4",
        "symfony/inflector": "^3.4 || ^4.4",
        "symfony/property-access": "^3.4 || ^4.4",
        "symfony/cache": "^3.4 || ^4.4"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\ETL\\PHPSpecExtension\\FastMap\\": "src/FastMap/",
            "Kiboko\\Component\\ETL\\PHPSpecExtension\\Metadata\\": "src/Metadata/",
            "Kiboko\\Component\\ETL\\PHPSpecExtension\\DataProvider\\": "src/DataProvider/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Kiboko\\Component\\ETL\\PHPSpecExtension\\": "spec/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
